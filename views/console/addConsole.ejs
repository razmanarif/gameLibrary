<h1>Add a console</h1>
<form action="/add_console" method="POST">
    <div>
    <label for="name">
        <input type="text" placeholder="Console Name" name="name">
    </label>
    </div>
    <div>
        <label for="manufacturer">
            <input type="text" placeholder="Console Manufacturer" name="manufacturer">
        </label>
    </div>
    <div>
        <label for="dateBought">
            <input type="date" name="dateBought">
        </label>
    </div>
    <button>Add Console</button>
</form>